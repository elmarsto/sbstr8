{
  "name": "sbstr8",
  "version": "0.0.2",
  "description": "A maximalist, ultra-modern, opinionated scaffold for rich, interactive storytelling, journalism and blogging",
  "scripts": {
    "dev": "npx next",
    "build": "npx next build",
    "start": "npx next start",
    "type-check": "npx tsc --noEmit",
    "lint": "npx eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "npx prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "npx husky install",
    "storybook": "storybook dev -s ./public -p 6006",
    "build-storybook": "storybook build -s ./public",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmarsto/sbstr8.git"
  },
  "keywords": [
    "sbstr8",
    "substrate",
    "nextjs",
    "storytelling",
    "blog"
  ],
  "author": "@elmarsto",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/elmarsto/sbstr8/issues"
  },
  "homepage": "https://github.com/elmarsto/sbstr8#readme",
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
